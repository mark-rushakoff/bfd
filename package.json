{
  "name": "bfd",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/mark-rushakoff/bfd",
  "license": "MIT",
  "scripts": {
    "postinstall": "node node_modules/bower/bin/bower install",
    "lint": "eslint .",
    "build": "npm run clean && env UV_THREADPOOL_SIZE=100 webpack -p --config webpack.production.config.js",
    "build:test": "npm run clean:test && env UV_THREADPOOL_SIZE=100 webpack --config webpack.test.config.js",
    "build:dev:once": "npm run clean:dev && env UV_THREADPOOL_SIZE=100 webpack",
    "build:dev": "npm run build:dev:once -- --watch",
    "clean": "rm -rf dist",
    "clean:dev": "rm -rf dev",
    "clean:test": "rm -rf built_specs",
    "serve:dev": "webpack-dev-server --content-base dev",
    "test": "npm run build:test -- -p && testem ci",
    "test:watch": "npm run build:test -- --watch",
    "test:dev": "testem"
  },
  "devDependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.5.8",
    "babel-loader": "^5.1.4",
    "bower": "^1.4.1",
    "core-js": "^0.9.17",
    "css-loader": "^0.14.5",
    "eslint": "^0.23.0",
    "eslint-loader": "^0.14.0",
    "eslint-plugin-react": "^2.6.3",
    "express": "^4.12.4",
    "hanson": "^1.1.1",
    "hson-loader": "^1.0.0",
    "html-webpack-plugin": "^1.5.2",
    "jasmine-core": "^2.3.4",
    "node-sass": "^3.2.0",
    "null-loader": "^0.1.1",
    "path-is-absolute": "^1.0.0",
    "sass-loader": "^1.0.2",
    "style-loader": "^0.12.3",
    "testem": "^0.8.3",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "babel-runtime": "^5.5.8",
    "bloody-keymirror": "^1.0.0",
    "classnames": "^2.1.2",
    "file-loader": "^0.8.4",
    "immutable-store": "^0.4.2",
    "react": "^0.13.3",
    "react-stampit": "^0.6.1",
    "stampit": "^2.0.3",
    "url-loader": "^0.5.6"
  }
}
